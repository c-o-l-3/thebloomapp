# Journey Visualizer Environment Variables
# Copy this file to .env and fill in your values

# Data Source Configuration
# Options: 'api' (PostgreSQL - recommended), 'airtable' (legacy), 'local' (development)
VITE_DATA_SOURCE=api

# PostgreSQL API Configuration (New - Recommended)
VITE_API_URL=http://localhost:3001/api

# Airtable Configuration (Legacy - for migration only)
VITE_AIRTABLE_API_KEY=your_airtable_personal_access_token
VITE_AIRTABLE_BASE_ID=your_airtable_base_id

# GoHighLevel Configuration
VITE_GHL_API_KEY=your_gohighlevel_api_key

# Optional: API Endpoints (defaults are set in services)
# VITE_AIRTABLE_API_URL=https://api.airtable.com/v0
# VITE_GHL_API_URL=https://api.gohighlevel.com/v2

# Optional: Feature Flags
# VITE_ENABLE_REALTIME_UPDATES=true
# VITE_ENABLE_NOTIFICATIONS=true
